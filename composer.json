{
    "name": "zareismail/maintainable",
    "description": "A Laravel Nova tool for the Hafiz package.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "zareismail/nova-cascade": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Maintainable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Maintainable\\MaintainableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
